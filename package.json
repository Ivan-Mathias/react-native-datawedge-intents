{
  "name": "react-native-datawedge-intents",
  "version": "0.2.0",
  "description": "Forked React Native Android module for Zebra DataWedge. Supports Android 8+ through Android 14+ (API 34) with fixed BroadcastReceiver exports.",
  "main": "./lib/commonjs/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "bob build"
  },
  "keywords": [
    "react",
    "react-native",
    "zebra",
    "dataWedge",
    "android 13",
    "MC33",
    "MC3300",
    "scanning"
  ],
  "author": "Darryn Campbell",
  "contributors": [
    {
      "name": "Ivan Mathias",
      "email": "ivanmathiassilva@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/react-native-datawedge-intents.git"
  },
  "bugs": {
    "url": "https://github.com/Ivan-Mathias/react-native-datawedge-intents.git/issues"
  },
  "peerDependencies": {
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-native": "^0.72.8",
    "react-native-builder-bob": "^0.40.16",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "source": "./src/index.js",
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
